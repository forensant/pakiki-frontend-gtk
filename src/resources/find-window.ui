<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <requires lib="gtk" version="4.14"/>
  <template class="PakikiFindWindow" parent="GtkWindow">
    <property name="modal">True</property>
    <property name="title">Find Request</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkSearchBar">
            <property name="child">
              <object class="GtkBox">
                <property name="spacing">12</property>
                <child>
                  <object class="GtkSearchEntry" id="search_entry">
                    <property name="hexpand">True</property>
                    <property name="hexpand-set">True</property>
                    <property name="placeholder-text">Filter Requests</property>
                    <signal name="search-changed" handler="set_find_filter"/>
                  </object>
                </child>
                <child>
                  <object class="GtkCheckButton" id="checkbutton_negative_filter">
                    <property name="label">_Negative Filter</property>
                    <property name="use-underline">True</property>
                    <signal name="toggled" handler="set_find_filter"/>
                  </object>
                </child>
                <child>
                  <object class="GtkCheckButton" id="checkbutton_exclude_resources">
                    <property name="active">True</property>
                    <property name="label">_Exclude Resources (Images, Stylesheets, etc)</property>
                    <property name="use-underline">True</property>
                    <signal name="toggled" handler="set_find_filter"/>
                  </object>
                </child>
              </object>
            </property>
            <property name="search-mode-enabled">True</property>
          </object>
        </child>
        <child>
          <object class="GtkViewport" id="viewport_requests"/>
        </child>
        <child>
          <object class="GtkSeparator"/>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin-bottom">12</property>
            <property name="margin-end">12</property>
            <property name="margin-start">12</property>
            <property name="margin-top">12</property>
            <property name="spacing">6</property>
            <child>
              <object class="GtkLabel">
                <property name="hexpand">True</property>
                <property name="hexpand-set">True</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_cancel">
                <property name="label">Cancel</property>
                <property name="use-underline">True</property>
                <signal name="clicked" handler="on_button_cancel_clicked"/>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_ok">
                <property name="label">OK</property>
                <signal name="clicked" handler="on_button_ok_clicked"/>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <requires lib="gtk" version="4.12"/>
  <template class="PakikiIntercept" parent="GtkBox">
    <child>
      <object class="GtkPaned">
        <property name="margin-bottom">18</property>
        <property name="margin-end">18</property>
        <property name="margin-start">18</property>
        <property name="margin-top">18</property>
        <property name="orientation">vertical</property>
        <property name="position">200</property>
        <property name="position-set">True</property>
        <child>
          <object class="GtkGrid">
            <child>
              <object class="GtkLabel">
                <property name="label">Intercept</property>
                <property name="margin-end">12</property>
                <property name="xalign">1.0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">0</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="checkbox_intercept_to_server">
                <property name="label">Requests</property>
                <signal name="toggled" handler="on_checkbox_intercept_to_server_toggled"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">0</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="checkbox_intercept_to_browser">
                <property name="label">Responses</property>
                <signal name="toggled" handler="on_checkbox_intercept_to_browser_toggled"/>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Queued Requests</property>
                <property name="margin-end">12</property>
                <property name="margin-top">12</property>
                <property name="xalign">1.0</property>
                <property name="yalign">0.0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <property name="has-frame">True</property>
                <property name="hexpand">True</property>
                <property name="hexpand-set">True</property>
                <property name="margin-bottom">12</property>
                <property name="margin-top">12</property>
                <property name="vexpand">True</property>
                <property name="vexpand-set">True</property>
                <child>
                  <object class="GtkColumnView" id="list_requests">
                    <property name="hexpand">True</property>
                    <property name="hexpand-set">True</property>
                    <property name="vexpand">True</property>
                    <property name="vexpand-set">True</property>
                  </object>
                </child>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin-bottom">12</property>
            <property name="margin-top">12</property>
            <property name="orientation">vertical</property>
            <property name="spacing">12</property>
            <child>
              <object class="GtkBox">
                <property name="spacing">12</property>
                <child>
                  <object class="GtkLabel" id="label_edit_request">
                    <property name="hexpand">True</property>
                    <property name="hexpand-set">True</property>
                    <property name="xalign">0.0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="button_drop">
                    <property name="label">_Drop</property>
                    <property name="use-underline">True</property>
                    <signal name="activate" handler="on_button_drop_activate"/>
                    <signal name="clicked" handler="on_button_drop_activate"/>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="button_intercept_response">
                    <property name="label">Forward and _Intercept Response</property>
                    <property name="use-underline">True</property>
                    <signal name="activate" handler="on_button_intercept_response_activate"/>
                    <signal name="clicked" handler="on_button_intercept_response_activate"/>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="button_forward">
                    <property name="label">_Forward</property>
                    <property name="use-underline">True</property>
                    <signal name="activate" handler="on_button_forward_activate"/>
                    <signal name="clicked" handler="on_button_forward_activate"/>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkPaned">
                <property name="position">250</property>
                <property name="position-set">True</property>
                <child>
                  <object class="GtkBox" id="box_original_request">
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkScrolledWindow" id="scrolled_window_text_request">
                        <property name="has-frame">True</property>
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>
                        <property name="vexpand">True</property>
                        <property name="vexpand-set">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow" id="scrolled_window_hex_request">
                        <property name="has-frame">True</property>
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>
                        <property name="vexpand">True</property>
                        <property name="vexpand-set">True</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkScrolledWindow" id="scrolled_window_text_requestresponse">
                        <property name="has-frame">True</property>
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>
                        <property name="vexpand">True</property>
                        <property name="vexpand-set">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow" id="scrolled_window_hex_requestresponse">
                        <property name="has-frame">True</property>
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>
                        <property name="vexpand">True</property>
                        <property name="vexpand-set">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

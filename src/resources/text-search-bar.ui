<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <requires lib="gtk" version="4.12"/>
  <template class="PakikiTextSearchBar" parent="GtkBox">
    <property name="vexpand-set">True</property>
    <child>
      <object class="GtkSearchBar" id="search_bar">
        <property name="show-close-button">True</property>
        <property name="vexpand-set">True</property>
        <child>
          <object class="GtkBox">
            <property name="hexpand">True</property>
            <property name="spacing">6</property>
            <property name="vexpand-set">True</property>
            <child>
              <object class="GtkLabel">
                <property name="hexpand">True</property>
              </object>
            </child>
            <child>
              <object class="GtkSearchEntry" id="search_entry_text">
                <property name="hexpand">True</property>
                <property name="hexpand-set">True</property>
                <signal name="next-match" handler="on_search_entry_text_next_match"/>
                <signal name="previous-match" handler="on_search_entry_text_previous_match"/>
                <signal name="search-changed" handler="on_search_entry_text_search_changed"/>
                <signal name="stop-search" handler="on_search_entry_text_stop_search"/>
                <signal name="activate" handler="on_search_entry_text_next_match"/>
              </object>
            </child>
            <child>
              <object class="GtkDropDown" id="dropdown_format">
                <property name="model">
                  <object class="GtkStringList">
                    <property name="strings">ASCII
Hex</property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_search_up">
                <property name="icon-name">go-up</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_search_down">
                <property name="icon-name">go-down</property>
              </object>
            </child>
            <child>
              <object class="GtkSpinner" id="spinner"/>
            </child>
            <child>
              <object class="GtkLabel" id="label_count">
                <property name="hexpand">True</property>
                <property name="hexpand-set">True</property>
                <property name="xalign">0.0</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
